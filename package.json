{
  "name": "@nexxspace/campsite",
  "version": "1.0.3",
  "description": "Script to help improve test coverage",
  "main": "index",
  "bin": {
    "campsite": "dist/index.js"
  },
  "files": ["dist/**/*"],
  "scripts": {
    "start": "node dist/index.js",
    "start:baseline": "node dist/index.js --baseline",
    "test": "jest --coverage",
    "build": "npx babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "prepare": "npx babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "keywords": [
    "jest",
    "testing"
  ],
  "author": "Peter Yu",
  "license": "ISC",
  "dependencies": {
    "colors": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.7",
    "babel-jest": "^26.1.0",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeterYu/campsite.git"
  },
  "bugs": {
    "url": "https://github.com/PeterYu/campsite/issues"
  },
  "homepage": "https://github.com/PeterYu/campsite#readme"
}
